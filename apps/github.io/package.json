{
  "name": "@apps/github.io",
  "private": true,
  "type": "module",
  "scripts": {
    "typegen": "dotenvx-typegen -f ../../.env -i 'VITE_'",
    "typegen:production": "dotenvx-typegen -f ../../.env.production -i 'VITE_'",
    "dev": "dotenvx run -f ../../.env --overload -- react-router dev",
    "dev:production": "dotenvx run -f ../../.env.production --overload -- react-router dev",
    "typecheck": "react-router typegen && tsc -b",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@react-router/node": "7.11.0",
    "@resettle/design": "workspace:^",
    "@scalar/api-reference-react": "0.8.15",
    "isbot": "5.1.32",
    "lucide-react": "0.562.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-router": "7.11.0"
  },
  "devDependencies": {
    "@react-router/dev": "7.11.0",
    "@tailwindcss/vite": "4.1.18",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "vite": "7.3.0",
    "vite-tsconfig-paths": "6.0.3"
  }
}
